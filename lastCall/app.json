{
  "expo": {
    "name": "LastCall",
    "slug": "lastCall",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo3.png",
    "scheme": "lastcall",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "Employee scheduling for bars, restaurants, and service businesses",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lastcal.lastCall",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSUserTrackingUsageDescription": "We respect your privacy. This identifier helps us provide a better experience.",
        "NSLocationWhenInUseUsageDescription": "Location is never collected or used by this app.",
        "NSCameraUsageDescription": "Camera access is never requested by this app.",
        "NSPhotoLibraryUsageDescription": "Photo library access is never requested by this app."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo3.png",
        "backgroundColor": "#1a1f3a"
      },
      "package": "com.lastcal.lastCall",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/logo3.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo3.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-web-browser",
      "expo-font",
      "@react-native-community/datetimepicker"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "cfb64a22-48e1-4fff-a2ff-f022f9d8274c"
      }
    },
    "owner": "lastcal"
  }
}
